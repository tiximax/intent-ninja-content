# Example environment variables for development (copy to .env.local and fill values)
# Do NOT commit real secrets.

# Supabase configuration
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Feature flags
# When true, content generation will use mock fallback if backend is unavailable
VITE_USE_MOCK_CONTENT=true

# Optional providers
# SERPAPI_API_KEY=
# Enable SerpApi provider via Edge Function
VITE_ENABLE_SERPAPI_PROVIDER=false
